{
    "name": "rhyslees/nova-impersonating-banner",
    "description": "An impersonating banner for Nova 4",
    "license": "MIT",
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "laravel/nova": "^4.0",
        "livewire/livewire": "^2.5"
    },
    "require-dev": {
        "laravel/pint": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "RhysLees\\NovaImpersonatingBanner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RhysLees\\NovaImpersonatingBanner\\NovaImpersonatingBannerServiceProvider"
            ]
        }
    },
    "scripts":{
        "format": [
            "vendor/bin/pint"
        ]
    }
}
